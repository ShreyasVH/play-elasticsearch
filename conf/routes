GET		/v1/books/:id			controllers.IndexController.get(request : Request, id : String)
GET		/v1/books			controllers.IndexController.getAll(request : Request)
GET		/search			controllers.IndexController.search(request : Request, author : String)
POST	/v1/books			controllers.IndexController.post(request : Request)
PUT	    /v1/books/:id			controllers.IndexController.put(request : Request, id: String)
DELETE	/v1/books/:id			controllers.IndexController.delete(request : Request, id: String)